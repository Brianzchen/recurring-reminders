{
  "name": "Recurring Reminders",
  "version": "1.0",
  "description": "Plugin to remind yourself of recurring events",
  "manifest_version": 2,
  "permissions": [
    "declarativeContent",
    "storage"
  ],
  "page_action": {
    "default_popup": "popup/index.html"
  },
  "chrome_url_overrides" : {
    "newtab": "newTab/index.html"
  }
}
